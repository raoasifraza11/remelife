[mysqld]
# Basic settings
user = raoasifraza
port = 3306
socket = /tmp/mysql.sock
pid-file = /tmp/mysql.pid
datadir = ~/mysql_data

# Security settings
skip-grant-tables = 0
skip-networking = 0
bind-address = 127.0.0.1

# Logging
log-error = ~/mysql_data/mysql_error.log
general_log = 0
slow_query_log = 0

# Performance
innodb_buffer_pool_size = 128M
max_connections = 100
query_cache_size = 32M

[mysql]
socket = /tmp/mysql.sock
user = raoasifraza

[mysqladmin]
socket = /tmp/mysql.sock
user = raoasifraza

[mysqldump]
socket = /tmp/mysql.sock
user = raoasifraza
